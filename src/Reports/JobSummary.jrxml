	<queryString>
		<![CDATA[SELECT
     purchaseorder.`job`,
     purchaseorder.`orderid` AS orderid,
     supplier.`companyname` AS companyname,
     purchaseorder.`total` AS po_total,
     purchaseorder.`bldg` AS bldg,
     purchaseorder.`status` AS status
FROM
     `supplier` supplier INNER JOIN `purchaseorder` purchaseorder ON supplier.`supplierid` = purchaseorder.`supplier`
where purchaseorder.job= $P!{id};]]>
	</queryString>
